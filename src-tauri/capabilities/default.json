{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "process:default",
    "dialog:default",
    "log:default",
    "axum:default",
    "pinia:default",
    "core:window:allow-show",
    "clipboard-manager:allow-read-text",
    "fs:allow-desktop-write"
  ]
}
